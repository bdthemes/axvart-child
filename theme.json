{
    "version": 2,
    "$schema": "https://schemas.wp.org/wp/6.3/theme.json",
    "settings": {
        "appearanceTools": true,
        "layout": {
            "contentSize": "1200px",
            "wideSize": "1440px"
        },
        "color": {
            "background": true,
            "custom": true,
            "customDuotone": true,
            "customGradient": true,
            "defaultDuotone": true,
            "defaultGradients": true,
            "defaultPalette": true,
            "duotone": [],
            "gradients": [],
            "palette": [
                {
                    "color": "#705CF6",
                    "name": "Light Blue",
                    "slug": "axvart-light-blue"
                },
                {
                    "color": "#BCEC00",
                    "name": "Light Green",
                    "slug": "axvart-light-green"
                },
                {
                    "color": "#1D1C1C",
                    "name": "Black",
                    "slug": "axvart-black"
                },
                {
                    "color": "#908E8E",
                    "name": "Gray",
                    "slug": "axvart-gray"
                },
                {
                    "color": "#838383",
                    "name": "Light Gray",
                    "slug": "axvart-light-gray"
                },
                {
                    "color": "#FFFFFF",
                    "name": "White",
                    "slug": "axvart-white"
                }
            ]
        },
        "shadow": {
            "defaultPresets": true,
            "presets": []
        },
        "spacing": {
            "blockGap": true,
            "units": ["%", "px", "em", "rem", "vh", "vw"],
            "customSpacingSize": true,
            "spacingScale": {
                "increment": 1.5,
                "mediumStep": 1.5,
                "operator": "*",
                "steps": 0,
                "unit": "rem"
            },
            "spacingSizes": [
                {
                    "size": "clamp(0.313rem, 1vw, 0.625rem)",
                    "slug": "axvart-extra-small",
                    "name": "Extra Small"
                },
                {
                    "size": "clamp(0.938rem, 2.3vw, 1.25rem)",
                    "slug": "axvart-small",
                    "name": "Small"
                },
                {
                    "size": "clamp(1.25rem, 3.5vw, 1.875rem)",
                    "slug": "axvart-medium",
                    "name": "Medium"
                },
                {
                    "size": "clamp(1.875rem, 6vw, 3.75rem)",
                    "slug": "axvart-normal",
                    "name": "Normal"
                },
                {
                    "size": "clamp(2.5rem, 7.3vw, 4.688rem)",
                    "slug": "axvart-large",
                    "name": "Large"
                },
                {
                    "size": "clamp(3.75em, 10.5vw, 6.25rem)",
                    "slug": "axvart-extra-large",
                    "name": "Extra Large"
                },
                {
                    "size": "clamp(4.375rem, 12.5vw, 8.125rem)",
                    "slug": "axvart-huge",
                    "name": "Huge"
                }
            ]
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
                    "name": "System Font",
                    "slug": "system-font"
                },
                {
                    "fontFamily": "Urbanist",
                    "slug": "urbanist",
                    "fontFace": [
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "normal",
                            "fontWeight": "100",
                            "src": ["file:./assets/fonts/urbanist_normal_100.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "normal",
                            "fontWeight": "200",
                            "src": ["file:./assets/fonts/urbanist_normal_200.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "normal",
                            "fontWeight": "300",
                            "src": ["file:./assets/fonts/urbanist_normal_300.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "src": ["file:./assets/fonts/urbanist_normal_400.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "normal",
                            "fontWeight": "500",
                            "src": ["file:./assets/fonts/urbanist_normal_500.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "normal",
                            "fontWeight": "600",
                            "src": ["file:./assets/fonts/urbanist_normal_600.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "normal",
                            "fontWeight": "700",
                            "src": ["file:./assets/fonts/urbanist_normal_700.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "normal",
                            "fontWeight": "800",
                            "src": ["file:./assets/fonts/urbanist_normal_800.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "normal",
                            "fontWeight": "900",
                            "src": ["file:./assets/fonts/urbanist_normal_900.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "italic",
                            "fontWeight": "100",
                            "src": ["file:./assets/fonts/urbanist_italic_100.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "italic",
                            "fontWeight": "200",
                            "src": ["file:./assets/fonts/urbanist_italic_200.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "italic",
                            "fontWeight": "300",
                            "src": ["file:./assets/fonts/urbanist_italic_300.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "italic",
                            "fontWeight": "400",
                            "src": ["file:./assets/fonts/urbanist_italic_400.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "italic",
                            "fontWeight": "500",
                            "src": ["file:./assets/fonts/urbanist_italic_500.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "italic",
                            "fontWeight": "600",
                            "src": ["file:./assets/fonts/urbanist_italic_600.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "italic",
                            "fontWeight": "700",
                            "src": ["file:./assets/fonts/urbanist_italic_700.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "italic",
                            "fontWeight": "800",
                            "src": ["file:./assets/fonts/urbanist_italic_800.ttf"]
                        },
                        {
                            "fontFamily": "Urbanist",
                            "fontStyle": "italic",
                            "fontWeight": "900",
                            "src": ["file:./assets/fonts/urbanist_italic_900.ttf"]
                        }
                    ]
                }
            ],
            "fontSizes": [
                {
                    "slug": "extra-small",
                    "size": "clamp(0.875rem, 2vw, 0.875rem)",
                    "name": "Extra Small"
                },
                {
                    "slug": "small",
                    "size": "clamp(0.875rem, 2vw, 1rem)",
                    "name": "Small"
                },
                {
                    "slug": "normal",
                    "size": "clamp(1.125rem, 3vw, 1.5rem)",
                    "name": "Normal"
                },
                {
                    "slug": "medium",
                    "size": "clamp(1.375rem, 4vw, 2rem)",
                    "name": "Medium"
                },
                {
                    "slug": "large",
                    "size": "clamp(1.75rem, 4vw, 2.625rem)",
                    "name": "Large"
                },
                {
                    "slug": "extra-large",
                    "size": "clamp(2rem, 5vw, 3.625rem)",
                    "name": "Extra large"
                },
                {
                    "slug": "huge",
                    "size": "clamp(2.188rem, 6.5vw, 4.375rem);",
                    "name": "Huge"
                }
            ],
            "customFontSize": true,
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": true,
            "lineHeight": true,
            "textColumns": true,
            "textDecoration": true,
            "textTransform": true,
            "dropCap": true,
            "fluid": true
        },
        "useRootPaddingAwareAlignments": true
    },
    "styles": {
        "blocks": {
            "core/navigation": {
                "color": {
                    "text": "var(--wp--preset--color--axvart-black)"
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontFamily": "var(--wp--preset--font-family--urbanist)",
                    "lineHeight": "clamp(1.4rem, 2vw, 1.5rem)",
                    "fontWeight": "500"
                },
                "elements": {
                    "link": {
                        ":hover": {
                            "color": {
                                "text": "var(--wp--preset--color--axvart-light-blue) !important"
                            }
                        },
                        ":focus": {
                            "color": {
                                "text": "var(--wp--preset--color--light-green) !important"
                            }
                        },
                        ":active": {
                            "color": {
                                "text": "var(--wp--preset--color--light-green) !important"
                            }
                        }
                    }
                }
            },
            "core/post-title": {
                "elements": {
                    "link": {
                        ":active": {
                            "color": {
                                "text": "var(--wp--preset--color--axvart-gray)"
                            },
                            "typography": {
                                "textDecoration": "none"
                            }
                        },
                        "typography": {
                            "textDecoration": "none",
                            "fontSize": "var(--wp--preset--font-size--large)"
                        }
                    }
                }
            },
            "core/post-content": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                },
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/post-excerpt": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                },
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/post-date": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "500"
                }
            },
            "core/post-author": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "500"
                }
            },
            "core/post-author-name": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "500"
                }
            },
            "core/post-navigation-link": {
                "elements": {
                    "link": {
                        ":hover": {
                            "color": {
                                "text": "var(--wp--preset--color--axvart-light-blue)"
                            },
                            "typography": {
                                "textDecoration": "underline"
                            }
                        },
                        ":focus": {
                            "color": {
                                "text": "var(--wp--preset--color--axvart-light-blue)"
                            },
                            "typography": {
                                "textDecoration": "underline"
                            }
                        },
                        ":active": {
                            "color": {
                                "text": "var(--wp--preset--color--axvart-light-blue)"
                            },
                            "typography": {
                                "textDecoration": "none"
                            }
                        },
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/comments-title": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--normal)",
                    "fontWeight": "700"
                }
            },
            "core/post-comments-form": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "700"
                }
            },
            "core/comment-edit-link": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--extra-small)"
                }
            },
            "core/comment-reply-link": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--extra-small)"
                }
            },
            "core/comments-pagination": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--extra-small)"
                },
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        },
                        "spacing": {
                            "padding": "5px"
                        }
                    }
                }
            },
            "core/query-title": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--normal)"
                },
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/query-pagination": {
                "typography": {
                    "fontWeight": "700"
                },
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/separator": {
                "color": {
                    "background": "var(--wp--preset--color--axvart-light-gray)"
                },
                "spacing": {
                    "margin": {
                        "top": "clamp(1.5rem, 2vw, 2rem)",
                        "bottom": "clamp(1.5rem, 2vw, 2rem)"
                    }
                }
            },
            "core/list": {
                "spacing": {
                    "margin": {
                        "top": "clamp(1.5rem, 2vw, 2rem)",
                        "bottom": "clamp(1.5rem, 2vw, 2rem)"
                    }
                }
            },
            "core/media-text": {
                "spacing": {
                    "margin": {
                        "top": "clamp(1.5rem, 2vw, 2rem)",
                        "bottom": "clamp(1.5rem, 2vw, 2rem)"
                    }
                }
            },
            "core/paragraph": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "400"
                }
            },
            "core/pullquote": {
                "border": {
                    "top": {
                        "color": "var(--wp--preset--color--axvart-light-blue)",
                        "style": "solid",
                        "width": "2px"
                    },
                    "bottom": {
                        "color": "var(--wp--preset--color--axvart-light-blue)",
                        "style": "solid",
                        "width": "2px"
                    }
                }
            },
            "core/quote": {
                "border": {
                    "left": {
                        "color": "var(--wp--preset--color--axvart-light-blue)",
                        "style": "solid",
                        "width": "4px"
                    }
                },
                "spacing": {
                    "padding": {
                        "left": "clamp(1rem, 2vw, 1.5rem)"
                    }
                }
            },

            "core/table": {
                "spacing": {
                    "margin": {
                        "top": "clamp(1.5rem, 2vw, 2rem)",
                        "bottom": "clamp(1.5rem, 2vw, 2rem)"
                    }
                }
            }
        },
        "elements": {
            "h1": {
                "typography": {
                    "lineHeight": "clamp(2.813rem, 6.5vw, 4.688rem)",
                    "fontSize": "var(--wp--preset--font-size--huge)",
                    "fontWeight": "700"
                }
            },
            "h2": {
                "typography": {
                    "lineHeight": "clamp(2.813rem, 5vw, 3.625rem)",
                    "fontSize": "var(--wp--preset--font-size--extra-large)",
                    "fontWeight": "700"
                }
            },
            "h3": {
                "typography": {
                    "lineHeight": "clamp(2.375rem, 4vw, 3.125rem)",
                    "fontSize": "var(--wp--preset--font-size--large)",
                    "fontWeight": "600"
                }
            },
            "h4": {
                "typography": {
                    "lineHeight": "clamp(2.188rem, 4vw, 2.813rem)",
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "600"
                }
            },
            "h5": {
                "typography": {
                    "lineHeight": "clamp(1.625rem, 3.5vw, 2rem)",
                    "fontSize": "var(--wp--preset--font-size--normal)",
                    "fontWeight": "600"
                }
            },
            "h6": {
                "typography": {
                    "lineHeight": "clamp(1.5rem, 2vw, 1.563rem)",
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "600"
                }
            },
            "button": {
                "typography": {
                    "lineHeight": "clamp(1.5rem, 2vw, 1.563rem)",
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "700"
                },
                "border": {
                    "radius": "100px"
                },
                "spacing": {
                    "padding": {
                        "top": "clamp(8px, 2.5vw, 14px)",
                        "right": "clamp(14px, 2.5vw, 30px)",
                        "bottom": "clamp(8px, 2.5vw, 14px)",
                        "left": "clamp(14px, 2.5vw, 30px)"
                    }
                },
                "color": {
                    "background": "var(--wp--preset--color--axvart-light-green)",
                    "text": "var(--wp--preset--color--axvart-black)"
                },
                ":hover": {
                    "color": {
                        "background": "#B1DE03",
                        "text": "var(--wp--preset--color--axvart-black)"
                    }
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--axvart-black)"
                },
                ":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--axvart-light-blue)"
                    }
                },
                "typography": {
                    "textDecoration": "none"
                }
            }
        },
        "color": {
            "background": "var(--wp--preset--color--axvart-white)",
            "text": "var(--wp--preset--color--axvart-black)"
        },
        "typography": {
            "fontSize": "var(--wp--preset--font-size--small)",
            "fontFamily": "var(--wp--preset--font-family--urbanist)",
            "lineHeight": "clamp(1.4rem, 2vw, 1.5rem)",
            "fontWeight": "500"
        },
        "spacing": {
            "margin": {
                "top": "0px",
                "right": "0px",
                "bottom": "0px",
                "left": "0px"
            },
            "padding": {
                "top": "0px",
                "right": "0px",
                "bottom": "0px",
                "left": "0px"
            },
            "blockGap": "1.5rem"
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header"
        },
        {
            "area": "footer",
            "name": "footer"
        }
    ]
}
